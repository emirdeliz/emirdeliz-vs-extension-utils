		"test:watch": "jest --watch",
		"lint:check": "eslint . --ext .ts --fix"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.3.14",
		"@swc/helpers": "^0.4.12",
		"@swc/jest": "^0.2.23",
		"@types/jest": "^29.2.2",
		"@types/node": "16.x",
		"@types/vscode": "^1.71.0",
		"@typescript-eslint/eslint-plugin": "^5.31.0",
		"@typescript-eslint/parser": "^5.31.0",
		"eslint": "^8.20.0",
		"jest": "^29.3.1",
		"ts-node": "^10.9.1",
		"typescript": "^4.7.4"
	}
}