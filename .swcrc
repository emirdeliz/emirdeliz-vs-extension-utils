{
	"$schema": "https://json.schemastore.org/swcrc",
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false
		},
		"target": "es2022",
		"loose": true
	},
	"module": {
		"type": "commonjs",
		"noInterop": true
	},
	"minify": false,
	"isModule": false,
	"exclude": ["node_modules", "dist", "src/test"]
}
